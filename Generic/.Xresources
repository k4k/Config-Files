! Disable URxvt Scrollbar
URxvt.scrollBar: false
! Clickable URLs in URxvt
URxvt.perl-ext-common: default,matcher,vtwheel,clipboard
URxvt.url-launcher: /usr/local/bin/midori
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4
! Set URxvt Background Color
URxvt*background: black
! Set URxvt Foreground color
URxvt*foreground: white
! Set Other URxvt colors
URxvt*color0: black
URxvt*color1: red3
URxvt*color2: green3
URxvt*color3: yellow3
URxvt*color4: blue2
URxvt*color5: magenta3
URxvt*color6: cyan3
URxvt*color7: gray90
URxvt*color8: grey50
URxvt*color9: red
URxvt*color10: green
URxvt*color11: yellow
URxvt*color12: blue
URxvt*color13: magenta
URxvt*color14: cyan
URxvt*color15: white
! Disable URxvt iso14755
URxvt.iso14755: False
! Set copy paste keys for URxvt
URxvt.keysym.Shift-Control-c: perl:clipboard:copy
URxvt.keysym.Shift-Control-v: perl:clipboard:paste

